{
  "branch": "master",
  "plugins": [
    "@semantic-release/commit-analyzer",
    ["@semantic-release/git", { "assets": ["src/**/*.csproj"], "message": "chore(release): ${nextRelease.version} [skip ci]" }],
    ["@semantic-release/exec", { "publishCmd": "scripts/publish-all.sh ${nextRelease.version}" }]
  ]
}
